import Cmp;
import IO;
import Monad;
import Math;


func fizzbuzz :: max': I8 -> current: I8 -> IO Unit
    | current > max' => exit 0
    | mod current 15 == 0 =>
        printStr "FizzBuzz"
        >> fizzbuzz max' (current + 1)
    | mod current 5 == 0 =>
        printStr "Buzz"
        >> fizzbuzz max' (current + 1)
    | mod current 3 == 0 =>
        printStr "Fizz"
        >> fizzbuzz max' (current + 1)
    | _ =>
        printI8 current
        >> fizzbuzz max' (current + 1)
    ;
